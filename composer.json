{
    "name": "digital-creative/nova-filepond",
    "description": "A Nova field for uploading File, Image and Video using filepond.",
    "keywords": [
        "file",
        "upload",
        "video",
        "laravel",
        "nova",
        "field",
    ],
    "authors": [
        {
            "name": "Rafael Milewski"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCreative\\Filepond\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCreative\\Filepond\\FilepondServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
